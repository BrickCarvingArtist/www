.topNav
	float left
	width 100%
	height 31px
	background-color rgb(234, 234, 234)
	border-bottom 1px solid rgb(201, 201, 201)
	.w1200
		position relative
		margin 0 auto
		z-index 2
	.supNav
		display block
		padding 0 10px
		text-align center
		font-size 12px
		line-height 30px
		.column
			icon-column 20px rgb(234, 234, 234) rgb(102, 102, 102)
		a
			color rgb(102, 102, 102)
	a.supNav
		float left
		height 30px
		color rgb(102, 102, 102)
		&:hover
			color rgb(64, 128, 255)
	div.supNav
		float right
		height 31px
		border 1px solid transparent
		&:hover
			background-color rgb(255, 255, 255)
			border-right-color rgb(201, 201, 201)
			border-left-color rgb(201, 201, 201)
			border-bottom-color rgb(255, 255, 255)
			i
				icon-column 20px rgb(255, 255, 255) rgb(64, 128, 255)
			> a
				color rgb(64, 128, 255)
			.subNav
				display block
		.subNav
			position absolute
			display none
			top 31px
			left 0
			padding 10px
			width 1200px
			overflow hidden
			background-color rgb(255, 255, 255)
			border-width 0 1px 1px
			border-style solid
			border-color rgb(201, 201, 201)
			text-align initial
			.section
				float left
				padding 2px 10px
				width 50%
				&:hover
					background-color rgb(234, 234, 234)
			h1
				font-size 12px
				line-height 2
				span
					padding 0 3px
					background-color rgb(255, 0, 0)
					border-radius 2px
					color rgb(255, 255, 0)
				strong
					margin-left 4px
					
					font-weight bold
				b
					margin-left 6px
			h2
				font-size 12px
				line-height 1.5
				span
					color rgb(102, 102, 102)
				b
					margin-right 3px
					font-weight bold
					color rgb(0, 128, 255)
					&:first-of-type:before
						margin-left 10px
						content "参数:"
						color rgb(0, 0, 0)
.header
	float left
	width 100%
	.w1200
		margin 0 auto
	.logoPart
		float left
		padding 10px 0
		width 100%
		.logo
			float left
			display block
			width 132px
			height 50px
			background-image url(/image/logo.png)
			background-position 50% 50%
			background-size contain
		.code
			float right
			width 50px
			height 50px
			background-image url(/image/code.png)
			background-size contain
.nav
	float left
	width 100%
	height 36px
	background-color rgb(36, 36, 36)
	.w1200
		margin 0 auto
	.all
		float left
		padding-left 10px
		width 180px
		height 36px
		line-height 36px
		.column
			float right 
			margin 3px 0
			icon-column 30px rgb(36, 36, 36) rgb(255, 255, 255)
		a
			font-size 18px
			font-weight bold
			color rgb(255, 255, 255)
	.navBtn
		position relative
		float right
		margin-left 32px
		width 54px
		text-align center
		font-size 16px
		line-height 36px
		color rgb(255, 255, 255)
		&:before
			position absolute
			top 0
			left 4px
			width 24px
			border-width 12px
			border-style solid
			border-color transparent transparent rgb(36, 36, 36) rgb(36, 36, 36) 
			border-radius 12px
			opacity 0
			transform rotate(10deg)
			content ""
		&:after
			position absolute
			top 0
			right 4px
			width 24px
			border-width 12px
			border-style solid
			border-color transparent rgb(36, 36, 36) rgb(36, 36, 36) transparent
			border-radius 12px
			opacity 0
			transform rotate(-10deg)
			content ""
		&:hover:before
		&:hover:after
			animation slide .25s forwards
@keyframes slide
	from
		top -10px
	to
		top -14px
		opacity 1