.topNav
	h = 30px
	float left
	width 100%
	height (h+1)
	background-color rgb(234, 234, 234)
	border-bottom 1px solid rgb(201, 201, 201)
	.w1200
		position relative
		margin 0 auto
		z-index 2
	.supNav
		display block
		padding 0 10px
		text-align center
		font-size 12px
		line-height h
		.column
			icon-column 20px rgb(234, 234, 234) rgb(102, 102, 102)
		a
			color rgb(102, 102, 102)
	a.supNav
		float left
		height h
		color rgb(102, 102, 102)
		&:hover
			color rgb(64, 128, 255)
	div.supNav
		float right
		height h+1
		border 1px solid transparent
		&:hover
			background-color rgb(255, 255, 255)
			border-right-color rgb(201, 201, 201)
			border-left-color rgb(201, 201, 201)
			border-bottom-color rgb(255, 255, 255)
			i
				icon-column 20px rgb(255, 255, 255) rgb(64, 128, 255)
			> a
				color rgb(64, 128, 255)
			.subNav
				display block
		.subNav
			position absolute
			display none
			top h
			left 0
			padding 10px
			width 1200px
			overflow hidden
			background-color rgb(255, 255, 255)
			border-width 0 1px 1px
			border-style solid
			border-color rgb(201, 201, 201)
			text-align initial
			.section
				float left
				padding 6px 10px
				width 50%
				&:hover
					background-color rgb(234, 234, 234)
			h1
				font-size 12px
				line-height 2
				span
					padding 0 3px
					background-color rgb(255, 0, 0)
					border-radius 2px
					color rgb(255, 255, 0)
				strong
					margin-left 4px
					
					font-weight bold
				b
					margin-left 6px
			h2
				font-size 12px
				line-height 1.5
				span
					color rgb(102, 102, 102)
				b
					margin-right 3px
					font-weight bold
					color rgb(0, 128, 255)
					&:first-of-type:before
						margin-left 10px
						content "参数:"
						color rgb(0, 0, 0)
.header
	float left
	width 100%
	.w1200
		margin 0 auto
	.logoPart
		float left
		padding 10px 0
		width 100%
		.logo
			float left
			display block
			width 132px
			height 50px
			background-image url(http://static.ikindness.cn/image/logo.png)
			background-position 50% 50%
			background-size contain
		.search
			float left
		.anchor
			float right
			margin-top 30px
			margin-left @margin-top
			font-size 16px
			font-weight bold
			line-height 20px
			color rgb(66, 66, 66)
			&:hover
				color rgb(64, 128, 255)